{
  "name": "amt_cache_get",
  "version": "0.0.1",
  "title": "AMT Retrieve Cached Response",
  "type": "flogo:activity",
  "ref": "github.com/lspwd2-klm/afklm-flogo/flogo-sandbox/activity/amt_cache_get",
  "description": "Loads proxied microservice response from cache",
  "author": "Aleks Yanchuk",
  "inputs":[
    {
      "name": "CacheKey",
      "type": "string",
      "required": true
    },
    {
      "name": "RedisHost",
      "type": "string",
      "required": true
    }
  ],
  "outputs": [
    {
      "name": "CacheBody",
      "type": "any"
    },
    {
      "name": "CacheHeaders",
      "type": "params"
    },
    {
      "name": "CacheCode",
      "type": "integer"
    },
    {
      "name": "Complete",
      "type": "boolean"
    }
  ]
}